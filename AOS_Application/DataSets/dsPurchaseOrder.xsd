                    <CommandText>SELECT        PO.PONUMBER, PO.PODATE, PO.POTYPE, PO.POTERMS, PO.POSTATUS, PO.CHANGEABLEPONUMBER, PO.SHIPNAME, PO.SHIPADDRESS1, PO.SHIPADDRESS2, 
