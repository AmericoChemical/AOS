                    <CommandText>SELECT        CUSTOMER.CUSTID, CUSTOMER.CUSTNAME, CUSTOMER.CUSTSHIPCITY, CUSTOMER.CUSTSHIPSTATEPROV, PRICELIST.PRODUCTID, 
