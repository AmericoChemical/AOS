                    <CommandText>SELECT     INVOICE.INVOICENUMBER, INVOICE.INVOICEDATE, INVOICE.SHIPMENTNUMBER, INVOICE.SHIPMENTDATE, INVOICE.CUSTID, 
                      INVOICE.BILLADDRESS1, INVOICE.BILLADDRESS2, INVOICE.BILLCITY, INVOICE.BILLSTATEPROV, INVOICE.BILLPOSTALCODE, 
