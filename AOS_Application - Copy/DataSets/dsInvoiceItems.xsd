                    <CommandText>SELECT     SHIPMENT.SHIPMENTNUMBER, SHIPMENTDETAIL.PRODUCTID, SHIPMENTDETAIL.PRODUCTDESC, SHIPMENTDETAIL.QTYCONTAINERS, 
